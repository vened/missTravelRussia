<div class="container" style="padding: 50px 0">
  <div class="row"></div>
  <%- @users.each do |user| %>
    <%- root_photo = user.photos.present? ? user.photos.find_by(root: true) : nil %>
    <%- if root_photo.present? %>
      <div class="col-md-3">
        <%= image_tag root_photo.photo_src.middle, class: "img-responsive" %>
      </div>
    <% end %>
  <% end %>
</div>

